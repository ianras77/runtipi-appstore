{
  "services": [
    {
      "name": "notebooklm",
      "image": "llmquest42/open-notebooklm:latest",
      "internalPort": 8000,
      "isMain": true,
      "environment": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/notebooklm",
          "containerPath": "/app/data",
          "readOnly": false
        }
      ]
    }
  ]
}

