{
  "name": "Anything-LLM",
  "id": "anything-llm",
  "available": true,
  "short_desc": "Chat with your docs; multi-LLM RAG app",
  "author": "ianras77",
  "version": "0.1.0",
  "tipi_version": 1,
  "source": "https://github.com/ianras77/anything-llm",
  "categories": ["ai"],
  "port": 3001,
  "exposable": true,
  "supported_architectures": ["amd64", "arm64"],
  "description": "Full-stack app to turn any document or content into context for LLMs with chat UI, agents, multi-user support, and more.",
  "form_fields": [
    {
      "type": "password",
      "label": "JWT Secret",
      "min": 20,
      "max": 100,
      "required": true,
      "env_variable": "JWT_SECRET"
    },
    {
      "type": "text",
      "label": "LLM Provider",
      "max": 50,
      "required": false,
      "env_variable": "LLM_PROVIDER",
      "default": "ollama"
    },
    {
      "type": "text",
      "label": "LLM Base URL",
      "max": 100,
      "required": false,
      "env_variable": "OLLAMA_BASE_PATH",
      "default": "http://192.168.1.224:11434"
    },
    {
      "type": "text",
      "label": "LLM Model (name)",
      "max": 50,
      "required": false,
      "env_variable": "OLLAMA_MODEL_PREF",
      "default": "llama2"
    },
    {
      "type": "text",
      "label": "Embedding Model",
      "max": 100,
      "required": false,
      "env_variable": "EMBEDDING_MODEL_PREF",
      "default": "nomic-embed-text:latest"
    }
  ],
  "dynamic_config": true
}

