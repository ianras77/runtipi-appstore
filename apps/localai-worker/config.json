{
  "app": {
    "name": "localai-worker",
    "type": "docker",
    "description": "Federated worker for LocalAI, connected to server at 192.168.1.12:8080",
    "icon": "https://example.com/worker-icon.png",
    "categories": ["ai", "worker", "federation"],
    "env": [
      {
        "key": "LOCALAI_SERVER_URL",
        "value": "http://192.168.1.12:8080"
      },
      {
        "key": "THREADS",
        "value": "12"
      },
      {
        "key": "TOKEN",
        "value": ""
      },
      {
        "key": "APP_PORT",
        "value": "8080"
      },
      {
        "key": "APP_DATA_DIR",
        "value": "/var/lib/localai-worker"
      },
      {
        "key": "APP_DOMAIN",
        "value": "worker.localai.example.com"
      },
      {
        "key": "LOCAL_DOMAIN",
        "value": "local"
      }
    ]
  }
}

