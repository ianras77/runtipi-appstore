{
  "$schema": "../app-info-schema.json",
  "name": "Gramps Web for Rasies",
  "available": true,
  "exposable": true,
  "dynamic_config": true,
  "port": 5000,
  "id": "grampsweb-rasies",
  "tipi_version": 12,
  "version": "24.12.2",
  "categories": ["data", "social"],
  "description": "Gramps Web is a web app for collaborative genealogy. It is based on and interoperable with Gramps, the leading open source genealogy desktop application. Gramps Web is free & open source software and puts your privacy and your control of your research data first.",
  "short_desc": "Web app for collaborative genealogy.",
  "author": "gramps-project",
  "source": "https://github.com/gramps-project/gramps-web",
  "form_fields": [
    {
      "type": "text",
      "label": "Gramps Tree Name",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "GRAMPSWEB_TREE"
    },
    {
      "type": "text",
      "label": "Email SMTP Host",
      "required": true,
      "env_variable": "GRAMPSWEB_EMAIL_HOST"
    },
    {
      "type": "number",
      "label": "Email SMTP Port",
      "default": 587,
      "required": true,
      "env_variable": "GRAMPSWEB_EMAIL_PORT"
    },
    {
      "type": "text",
      "label": "SMTP Username",
      "required": true,
      "env_variable": "GRAMPSWEB_HOST_USER"
    },
    {
      "type": "password",
      "label": "SMTP Password",
      "required": true,
      "env_variable": "GRAMPSWEB_HOST_PASSWORD"
    },
    {
      "type": "text",
      "label": "Default From Email",
      "required": true,
      "env_variable": "GRAMPSWEB_DEFAULT_FROM_EMAIL"
    }
  ],
  "default_env": {
    "POSTGRES_USER": "gramps_$(openssl rand -hex 4)",
    "POSTGRES_PASSWORD": "$(openssl rand -hex 16)"
  },
  "supported_architectures": ["arm64", "amd64"],
  "created_at": 1691943801422,
  "updated_at": 1735665701000
}

